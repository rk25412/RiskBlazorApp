                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Systemrole",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ClearConnection.updateSystemrole",
                        "parameters": [
                          {
                            "name": "ROLE_ID",
                            "value": "${parameters.ROLE_ID}"
                          },
                          {
                            "name": "Systemrole",
                            "value": "${systemrole}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${systemrole}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "ClearCovid.Models.ClearConnection.Systemrole",
                  "data": "${systemrole}",
                  "visible": "${systemrole != null}"
                }
              ]
            }
