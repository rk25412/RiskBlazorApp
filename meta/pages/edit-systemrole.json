  "events": {
    "load": [
      {
        "name": "ClearConnection.getSystemroleByROLE_ID",
        "parameters": [
          {
            "name": "ROLE_ID",
            "value": "${parameters.ROLE_ID}"
          }
        ],
        "then": [
          {
            "name": "systemrole",
            "type": "set",
            "value": "${result}",
            "valueType": "ClearCovid.Models.ClearConnection.Systemrole"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
