{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "NAME",
                              "name": "NAMELabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "NAME",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${consequence.NAME}"
                            },
                            {
                              "type": "required-validator",
                              "name": "NAMERequiredValidator",
                              "component": "NAME",
                              "text": "NAME is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Consequence Value",
                              "component": "CONSEQUENCE_VALUE",
                              "name": "CONSEQUENCE_VALUELabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "CONSEQUENCE_VALUE",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${consequence.CONSEQUENCE_VALUE}"
                            },
                            {
                              "type": "required-validator",
                              "name": "CONSEQUENCE_VALUERequiredValidator",
                              "component": "CONSEQUENCE_VALUE",
                              "text": "CONSEQUENCE_VALUE is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Consequence!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "ClearConnection.createConsequence",
                        "parameters": [
                          {
                            "name": "Consequence",
                            "value": "${consequence}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${consequence}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${consequence}",
                  "visible": "${consequence != null}",
                  "itemType": "ClearCovid.Models.ClearConnection.Consequence"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "consequence",
        "type": "set",
        "value": "new ClearCovid.Models.ClearConnection.Consequence(){}",
        "valueType": "ClearCovid.Models.ClearConnection.Consequence"
      }
    ]
  },
  "name": "Add Consequence",
  "access": "Authenticated",
  "layout": "Main"
}