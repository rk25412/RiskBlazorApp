  "events": {
    "load": [
      {
        "name": "ClearConnection.getPpevalueByPPE_ID",
        "parameters": [
          {
            "name": "PPE_ID",
            "value": "${parameters.PPE_ID}"
          }
        ],
        "then": [
          {
            "name": "ppevalue",
            "type": "set",
            "value": "${result}",
            "valueType": "ClearCovid.Models.ClearConnection.Ppevalue"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
